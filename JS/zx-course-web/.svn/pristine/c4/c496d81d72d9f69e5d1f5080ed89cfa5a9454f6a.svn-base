}
.study_con>a.in:hover{
	color: #ffffff;
	background: #56dcd1;
	cursor: default;
}

.study_con .attach{
	display: inline-block;
	width: 55px;
	line-height: 26px;
	font-size: 12px;
	color: #333;
	border: 1px solid #ededed;
	border-radius: 3px;
