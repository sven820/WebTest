}
.my_c_box{
	padding: 20px 20px 40px;
}
.my_c_info{
	padding: 25px;
	background: #fafafa;
	border: 1px solid #ededed;
}
.my_c_info_l{
	width: 500px;
}
.my_c_info_r{
	position: relative;
}
.my_c_info_r_tip{
	position: absolute;
	right: 0px;
	top: 38px;
	background: url(../../images/old/tips-icon.png) no-repeat;
	width: 198px;
	height: 48px;
	line-height: 52px;
	color: #ffffff;
	text-align: center;
	font-size: 14px;
}
.my_c_info_r a{
	background: url("../../images/old/download-icon.png") no-repeat;
	display: block;
	height: 37px;
	padding-left: 40px;
	line-height: 37px;
	font-size: 16px;
	color: #444444;
}
.my_c_info_l>p{
	color: #0dc2b3;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}
.my_c_info_l>p:first-child{
	font-size: 20px;
	color: #333333;
	margin-bottom: 5px;
}
.my_c_info_l>p span{
	margin-left: 25px;
}
.my_c_list li{
	padding: 20px 0 0 30px;
	background: url("../../images/old/my_c_icon.png") no-repeat 0 -346px;
}
.my_c_list li:last-child{
	background-position: 0 -882px;
}
.my_c_list li:last-child div.c_list_box{
	border-bottom: none;
}
.c_list_box{
	padding-bottom: 20px;
	border-bottom: 1px solid #ededed;
}
.c_content p{
	font-size: 12px;
	color: #999999;
}
.c_content p span{
	margin-left: 15px;
}
.c_content p:first-child{
	font-size: 14px;
	color: #333333;
	margin-bottom: 5px;
}
.c_content p:first-child span{
	color: #ff5e34;
	margin-right: 5px;
	margin-left: 0;
}
.list_box_top>a{
	display: block;
	float: right;
	width: 120px;
	line-height: 40px;
	font-size: 16px;
	color: #0dc2b3;
	border-radius: 3px;
	text-align: center;
}
.list_box_top>a.look{
	width: 118px;
	line-height: 38px;
	border: 1px solid #0dc2b3;
}
.list_box_top>a.look i{
	display: inline-block;
	width: 0;
	height: 0;
	border-top: 7px solid #0dc2b3;
	border-left: 5px solid transparent;
	border-right: 5px solid transparent;
	vertical-align: 2px;
	margin-left: 5px;
}
.list_box_top>a.look i.up{
	border-top: none;
	border-bottom: 7px solid #0dc2b3;
}
.list_box_top>a.look:hover{
	opacity: .7;
	filter: alpha(opacity=70);
}
.list_box_top>a.while{
	color: #ffffff;
	background: #0dc2b3;
}
.list_box_top>a.while:hover{
	color: #ffffff;
	background: #56dcd1;
}
.list_box_top>a.no{
	color: #fefefe;
	background: #d6d6d6;
	cursor: not-allowed;
}
.c_operate_box{
	font-size: 0;
	margin-top: 20px;
}
.c_operate_box a{
	display: inline-block;
	width: 63px;
	line-height: 26px;
	font-size: 12px;
	color: #333;
	border: 1px solid #ededed;
	border-radius: 3px;
	padding-left: 30px;
	background: url("../../images/old/my_c_icon.png") no-repeat 8px -142px;
	text-align: center;
	vertical-align: top;
	margin-right: 10px;
	margin-bottom: 6px;
}
.c_operate_box a+a{
	background-position: 8px -178px;
}
/*.c_operate_box a+a+a{
	background-position: 8px -215px;
}
.c_operate_box a+a+a+a{
	background-position: 8px -252px;
}*/
.c_operate_box a:hover{
	color: #0dc2b3;
	border-color: #0dc2b3;
}
.c_operate_box a.dis{
	color: #999999;
	background-position: 8px -1569px;
}
.c_operate_box a+a.dis{
	background-position: 8px -1605px;
}
.c_operate_box a+a+a.dis{
	background-position: 8px -1642px;
}
.c_operate_box a+a+a+a.dis{
	background-position: 8px -1679px;
}
.c_operate_box a.dis:hover{
	border-color: #ededed;
	cursor: default;
}
.look_back_video>p{
	color: #0dc2b3;
	margin-top: 20px;
	margin-bottom: 12px;
}
.back_video{
	margin-left: -18px;
}
.back_video dd{
	float: left;
	margin-left: 18px;
}
.back_video dd a{
	display: block;
	position: relative;
	width: 122px;
	height: 82px;
}
.back_video dd a img{
	width: 100% !important;
	height: 100% !important;
}
.back_video dd p{
	font-size: 12px;
	color: #666666;
	margin-top: 15px;
}
.back_video dd a i{
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: #000000;
	opacity: .5;
	filter: alpha(opacity=50);
}
.back_video dd a:after{
	content: '';
	display: block;
	position: absolute;
	left: 50%;
	top: 50%;
	width: 0;
	height: 0;
	border-left: 20px solid #0dc2b3;
	border-top: 12px solid transparent;
	border-bottom: 12px solid transparent;
	margin-left: -10px;
	margin-top: -12px;
}

