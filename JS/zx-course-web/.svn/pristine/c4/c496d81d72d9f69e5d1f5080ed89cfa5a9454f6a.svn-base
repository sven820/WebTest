.sel_course_box{
	background: #ffffff;
	border-bottom: 1px solid #ededed;
}
.sel_course_main{
	width: 1000px;
	font-size: 14px;
	color: #333333;
	padding-top: 20px;
	padding-bottom: 20px;
	margin: 0 auto;
}
.course_single{
	padding-top: 10px;
	border-top: 1px dashed #ededed;
}
.sel_course_main div.course_single:first-child{
	border-top: none;
}
.course_single>span{
	display: block;
	float: left;
	width: 50px;
	line-height: 26px;
	color: #999999;
	text-align: left;
}
.select_kind{
	float: left;
	width: 950px;
}
.select_kind li{
	float: left;
	margin-bottom: 10px;
	margin-right: 5px;
}
.select_kind li a{
	display: block;
	min-width: 38px;
	padding: 0 10px;
	line-height: 26px;
	color: #333333;
	border-radius: 3px;
	text-align: center;
}
.select_kind li a:hover,.select_kind li a.on{
	color: #ffffff;
	background: #0dc2b3;
}
.hover_shadow{
	-webkit-transition: all .2s linear;
	transition: all .2s linear;
}
.hover_shadow:hover{
	-webkit-box-shadow: 0 15px 30px rgba(0,0,0,0.1);
	box-shadow: 0 15px 30px rgba(0,0,0,0.1);
	-webkit-transform: translate3d(0, -2px, 0);
	transform: translate3d(0, -2px, 0)
}
.special-type{
	width: 1000px;
	margin: 30px auto 0;
}
.special-type .item{
	color:#666;
	font-size: 14px;
	display: inline-block;
	padding: 5px 15px;
	cursor: pointer;
	background: #fff;
	border: solid 1px #ededed;
}
.special-type .item:hover,.item.on{
	color: #fff;
	background: #0dc2b3;
}
.special-type .dash{
	-moz-border-top-left-radius:6px;
	-moz-border-bottom-left-radius:6px;
	-webkit-border-top-left-radius:6px;
	-webkit-border-bottom-left-radius:6px;
	border-top-left-radius:6px;
	border-bottom-left-radius:6px;
}
.special-type .break{
	margin-left: -4px;
	border-left: none;
	-moz-border-top-right-radius:6px;
	-moz-border-bottom-right-radius:6px;
	-webkit-border-top-right-radius:6px;
	-webkit-border-bottom-right-radius:6px;
	border-top-right-radius:6px;
	border-bottom-right-radius:6px;
}
.optional_curse{
	width: 1000px;
	margin: 30px auto 0;
	overflow: hidden;
}
.optional_curse ul{
	margin-left: -16px;
}
.optional_curse ul li{
