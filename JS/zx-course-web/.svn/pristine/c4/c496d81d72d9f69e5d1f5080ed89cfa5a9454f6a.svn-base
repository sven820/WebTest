/*班课推荐*/
.recommend .head {
	height: 50px;
	line-height: 50px;
	margin: 0 20px;
	background: #f5f5f5;
}
.recommend .head .tip {
	font-size: 20px;
	color: #333;
	margin-left: 20px;
	position: relative;
}
.recommend .head .hot {
	display: block;
	position: absolute;
	top: -2px;
	left: 185px;
	min-width: 15px;
	height: 18px;
	line-height: 18px;
	padding: 0 2px;
	font-size: 12px;
	font-style: normal;
	color: #ffffff;
	background: #ff5e34;
	border-radius: 3px;
	white-space: nowrap;
	text-align: center;
}
.recommend .head .hot:before {
	content: '';
	display: block;
	position: absolute;
	top: 18px;
	left: 6px;
	width: 0;
	height: 0;
	border-top: 3px solid #ff5e34;
	border-right: 5px dashed transparent;
}
.recommend .head .more {
	display: inline-block;
	float: right;
	margin-right: 20px;
	color: #666;
}
.recommend .head .more:hover {
	color: #0dc2b3;
}
.recommend_curse {
	width: 704px;
	margin: 20px auto 0;
	overflow: hidden;
}
.recommend_curse ul {
	padding-left: 20px;
}
.recommend_curse li:first-child {
	margin-left: 0px;
}
.recommend_curse li {
	float: left;
	width: 210px;
	background: #ffffff;
	margin-left: 16px;
	margin-bottom: 30px;
	margin-top: 2px;
}
.recommend_curse .pll_pic {
	display: block;
	position: relative;
	width: 210px;
	height: 160px;
	overflow: hidden;
}
