}
.t_col p:first-child span{
	margin-right: 10px;
}
.lession-title{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.study_con>span{
	display: block;
	float: left;
	line-height: 26px;
	font-size: 12px;
	color: #333;
	padding-left: 30px;
	background: url("../../images/old/my_c_icon.png") no-repeat 8px -142px;
	margin-top: 7px;
}
.study_con .in{
	display: block;
	float: right;
	width: 120px;
