	line-height: 40px;
	color: #0dc2b3;
	border-radius: 3px;
	text-align: center;
	color: #fefefe;
	background: #d6d6d6;
	cursor: not-allowed;
}
.no-lession-tip{
	text-align: center;
	margin-top: 10%;
	font-size: 18px;
	color: grey;
}
.scale img{
	position: relative;
	left: 0px;
	-webkit-transition: all 1s ease;
	-o-transition: all 1s ease;
	transition: all 1s ease;
	transform: scale(1.0);
}
.scale:hover img{
	position: relative;
	transform: scale(1.06);
}
