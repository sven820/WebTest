body{
	background: #f8f8f8;
}
.pll_curse_tab{
	padding: 15px 0;
}
.pll_curse_tab li{
	float: left;
}
.pll_curse_tab li.line{
	width: 1px;
	height: 14px;
	background: #bfbfbf;
	margin: 3px 25px 0;
}
.pll_curse_tab li a{
	display: block;
	line-height: 20px;
	color: #333333;
}
.pll_curse_tab li a:hover,.pll_curse_tab li a.on{
	color: #0dc2b3;
}
.pll_curse_tab.wid{
	border-bottom: 1px solid #e9e9e9;
}
.pll_curse_tab.wid ul{
	width: 1000px;
	margin: 0 auto;
}
.new_my_course{
	width: 1000px;
	font-size: 14px;
	color: #333333;
	background: #ffffff url("../../images/old/my_c_bg.png") repeat-y left top;
	margin: 20px auto 30px;
}
.my_c_center{
	float: left;
	width: 249px;
	height: 100%;
}
.my_c_warp{
	float: left;
	width: 755px;
}
.u_center_img{
	width: 100px;
	height: 100px;
	border-radius: 50%;
	margin: 25px auto 20px;
}
.u_center_img img{
	width: 100% !important;
	height: 100% !important;
	border-radius: 50%;
}
.u_center_name{
	font-size: 16px;
	color: #999999;
	text-align: center;
}
.u_center_name span{
	color: #0dc2b3;
}
.u_had_learn{
	text-align: center;
	margin-top: 15px;
}
.u_had_learn span{
	display: inline-block;
	line-height: 24px;
	padding: 0 5px;
	font-size: 12px;
	color: #ffffff;
	background: #0dc2b3;
	border-radius: 3px;
}
.c_account_tab{
	margin-top: 50px;
	margin-bottom: 50px;
}
.c_account_tab dd a{
	display: block;
	line-height: 80px;
	font-size: 16px;
	color: #666666;
	padding-left: 110px;
	background: url("../../images/old/my_c_icon.png") no-repeat 32px -58px;
}
.c_account_tab dd:first-child a{
	background-position: 35px 18px;
}
.c_account_tab dd a:hover{
	color: #0dc2b3;
}
.c_account_tab dd a.on{
	color: #0dc2b3;
